{
    "name": "silveton/acl",
    "description": "Package to A.C.L in laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "danilocarsan",
            "email": "danilocarsan@gmail.com"
        }
    ],
    "require": {
        "php" : "^7.2.5|^8.0",
        "illuminate/container": "^6.0|^7.0|^8.0",
        "illuminate/contracts": "^6.0|^7.0|^8.0",
        "illuminate/database": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Silveton\\Acl\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Silveton\\Acl\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Silveton\\Acl\\AclServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true

}
